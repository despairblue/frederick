{
  "extends": "tslint:recommended",
  "rules": {
    "no-string-literal": [ false ],
    "no-console": [ false ],
    "no-unused-expression": [ false ],
    "only-arrow-functions": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "never"
    ]
  }
}
