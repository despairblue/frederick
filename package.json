{
  "name": "frederick",
  "version": "0.0.1-1",
  "description": "",
  "main": "build/main.js",
  "types": "build/main.d.ts",
  "scripts": {
    "dev": "backpack",
    "build": "node scripts/build.js",
    "format": "tsfmt -r",
    "lint": "tslint src/**/*.ts && tslint tests/**/*.ts",
    "prepublish": "npm run build",
    "test": "run-s test:compile test:test",
    "test:watch": "run-p 'test:compile -- --watch' 'test:test -- --watch'",
    "test:debug": "run-s test:clean test:compile 'test:test -- --inspect --debug-brk'",
    "test:test": "ava",
    "test:clean": "rimraf tests/build",
    "test:compile": "tsc -p tests"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "conf": "^0.12.0",
    "inquirer": "^3.0.1",
    "meow": "^3.7.0"
  },
  "devDependencies": {
    "@types/conf": "^0.11.0",
    "@types/inquirer": "^0.0.32",
    "@types/meow": "^3.6.1",
    "@types/node": "^7.0.5",
    "@types/shelljs": "^0.7.0",
    "ava": "^0.18.2",
    "awesome-typescript-loader": "^3.0.4",
    "backpack-core": "^0.1.0",
    "npm-run-all": "^4.0.1",
    "rimraf": "^2.6.0",
    "shelljs": "^0.7.6",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6",
    "typescript-formatter": "^4.1.1"
  }
}
